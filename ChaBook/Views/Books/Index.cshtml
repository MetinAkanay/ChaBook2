            border-radius: 2px;
            margin-bottom: 12px;
            font-size: 12px;
        }

        .save {
            width: 100%;
            border-radius: 2px;
            border: 1px solid #000000;
            padding: 8px;
            margin: 12px 0;
            font-weight: 600;
        }

        .protect {
            margin-top: 12px;
            padding: 12px 12px 16px;
            background-color: #F7F7FF;
            border-radius: 2px;
            margin-bottom: 24px;
        }

            .protect p {
